@article{Ouellette2017,
abstract = {In this study we evaluated whether the sophistication of children's invented spellings in kindergarten was predictive of subsequent reading and spelling in Grade 1, while also considering the influence of well-known precursors. Children in their first year of schooling (mean age ? 66 months; N ? 171) were assessed on measures of oral vocabulary, alphabetic knowledge, phonological awareness, word reading and invented spelling; approximately 1 year later they were assessed on multiple measures of reading and spelling. Path modeling was pursued to evaluate a hypothesized unique, causal role of invented spelling in subsequent literacy outcomes. Results supported a model in which invented spelling contributed directly to concurrent reading along with alphabetic knowledge and phonological awareness. Longitu- dinally, invented spelling influenced subsequent reading, along with alphabetic knowledge while medi- ating the connection between phonological awareness and early reading. Invented spelling also influ- enced subsequent conventional spelling along with phonological awareness, while mediating the influence of alphabetic knowledge. Invented spelling thus adds explanatory variance to literacy outcomes not entirely captured by well-studied code and language-related skills. Keywords:},
author = {Ouellette, Gene P. and S{\'{e}}n{\'{e}}chal, Monique},
doi = {10.1037/dev0000179},
file = {:C$\backslash$:/Users/g517364/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ouellette, S{\'{e}}n{\'{e}}chal - 2017 - Invented spelling in kindergarten as a predictor of reading and spelling in grade 1 A new pathway to litera.pdf:pdf},
isbn = {1939-0599 (Electronic) 0012-1649 (Linking)},
issn = {00121649},
journal = {Developmental Psychology},
keywords = {Invented spelling,Literacy,Path models,Precursors,Reading,Spelling},
number = {1},
pages = {77--88},
pmid = {27617354},
title = {{Invented spelling in kindergarten as a predictor of reading and spelling in grade 1: A new pathway to literacy, or just the same road, kess known?}},
volume = {53},
year = {2017}
